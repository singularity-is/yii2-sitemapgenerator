{
  "name": "singularity/yii2-sitemapgenerator",
  "description": "Sitemap Generator for Yii2 framework",
  "keywords": [
    "singularity",
    "singularity solution",
    "yii",
    "yii2",
    "yii 2",
    "widget",
    "sitemapgenerator",
    "sitemap"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/singularity-is/yii2-sitemapgenerator",
  "authors": [
    {
      "name": "Singularity Solution",
      "email": "hello@singularity.is",
      "homepage": "https://singularity.is",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "icamys/php-sitemap-generator": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "singularity\\sitemapgenerator\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  }
}
